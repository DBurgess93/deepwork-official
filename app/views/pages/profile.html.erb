
  <% if user_signed_in? %>
  <%= render "shared/navbar_logged_in" %>
  <% else %>
  <%= render "shared/navbar_logged_out" %>
  <% end %>
<div class="hub-container">
  <div class="profile-left">
    <div>
      <%= render "components/stats_card" %>
    </div>
    <div>
      <%= render "shared/template_card" %>
    </div>

  </div>

  <div class="profile-right">
    <div>
      <%= render "shared/task_list" %>
    </div>
    <div class="button-flex">
      <%= link_to study_session_path(:user_id) do  %>
        <%= render "components/primary_button_profile" %>
      <% end %>
    </div>

  </div>
</div>
